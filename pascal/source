
import java.util.*; //importing Scanner class
 
class Pascal{ //opening of class

 
public static void printPascal(int a){   //fn to print pascal's  triangle

	for(int l = 1; l <= a; l++)   //l for the line
 
	{ 

		
	int C=1;  //C for each value in triangle

	for(int i = 1; i <= l; i++)
 
	{ 


		System.out.print(C+" ");
 
		C = C * (l - i) / i;   //to get next no: of line
 
	} 

	System.out.println(); 

	} 

} 

 
public static void main (String[] args) 
{ 
        Scnner sc =new Scanner(System.in);
	int a = sc.nextInt;   //inputing n no: of lines

	printPascal(a); //invoking print fn
 
}
 
} 
 

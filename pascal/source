
import java.io.*;
 
class pasCal
 { 


 
public static void printPascal(int a)
 
{ 

	for(int line = 1; line <= a; line++)
 
	{ 

		
	int C=1; 

	for(int i = 1; i <= line; i++)
 
	{ 


		System.out.print(C+" ");
 
		C = C * (line - i) / i;
 
	} 

	System.out.println(); 

	} 

} 

 
public static void main (String[] args) 
{ 

	int a = 5; 

	printPascal(n);
 
}
 
} 
 
